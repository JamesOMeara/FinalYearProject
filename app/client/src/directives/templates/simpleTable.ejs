<div>
    <table style="width:100%">
        <tr>
            <th>Firstname</th>
            <th>Lastname</th> 
            <th>Age</th>
        </tr>
        <tr ng-repeat="x in data">
            <td>{{ x.name }}</td>
            <td>{{ x.lastname }}</td>
            <td>{{ x.age }}</td>
            <td><button ng-click = "delete(  x.name , x.lastname , x.age )" >Delete</button></td>
        </tr>
        <tr>
            <td>
                <form>
                    <input type="text" ng-model="formData.firstname">
                </form>
            </td>
            <td>
                <form>
                    <input type="text" ng-model="formData.lastname">
                </form>
            </td>
            <td>
                <form>
                    <input type="text" ng-model="formData.age">
                </form>
            </td>
            <td>
                <button ng-click = "submit(formData.firstname, formData.lastname, formData.age)" >Submit</button>
            </td>
        </tr>
    </table>
</div>